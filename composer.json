{
  "name": "digital-drifter/laravel-chrome-logger",
  "description": "Laravel bridge for ChromePhp. Log PHP output to the Chrome console.",
  "type": "package",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1.0",
    "ccampbell/chromephp": "^4.1.0",
    "illuminate/config": "^5.6.15",
    "illuminate/support": "^5.6.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0.3"
  },
  "autoload": {
    "psr-4": {
      "DigitalDrifter\\LaravelChromeLogger\\": "src/"
    },
    "files": [
      "../../ccampbell/chromephp/ChromePhp.php",
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "DigitalDrifter\\LaravelChromeLogger\\LaravelChromeLoggerServiceProvider"
      ]
    }
  }
}
